    "isPrime :: Int -> Bool\n",
    "isPrime x = (x - l)  == 2\n",
    "    where l = length $ takeWhile (/=0) $ map (x `mod`) [2..(x-1)]\n",
    "findPrime :: Int -> Int\n",
    "findPrime n = last $ take n $ filter isPrime [1..]\n",
    "findPrime 10001"
   ]
